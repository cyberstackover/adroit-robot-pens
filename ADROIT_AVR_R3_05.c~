	SBIS 0x13,1
	LDI  R26,1
	LDS  R30,99
	ANDI R30,LOW(0x2)
	BRNE _0xBC
	LDI  R30,LOW(0)
	RJMP _0xBD
_0xBC:
	LDI  R30,LOW(1)
_0xBD:
