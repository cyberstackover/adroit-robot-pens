;void Stop(void)
; 0000 0293 {   PwmM2H = 0; PwmM2L = 0;
_Stop:
	LDI  R30,LOW(0)
	STS  135,R30
	STS  134,R30
; 0000 0294     PwmM1H = 0; PwmM1L = 0;
	STS  133,R30
	STS  132,R30
; 0000 0295 }
	RET
