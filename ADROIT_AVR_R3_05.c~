	RCALL _SetSudut
	STS  _dServo4,R30
	RJMP _0x2000004
;#pragma used-
;#pragma used+
;void SudutServo5(unsigned char posisi)
; 0000 0202 {   dServo5 = SetSudut(posisi);}
_SudutServo5:
;	posisi -> Y+0
	LD   R30,Y
	LDI  R31,0
	ST   -Y,R31
	ST   -Y,R30
	RCALL _SetSudut
	MOV  R12,R30
	RJMP _0x2000004
;#pragma used-
;#pragma used+
;void SudutServo6(unsigned char posisi)
; 0000 0206 {   dServo6 = SetSudut2(posisi);}
_SudutServo6:
;	posisi -> Y+0
	LD   R30,Y
	LDI  R31,0
	ST   -Y,R31
	ST   -Y,R30
	RCALL _SetSudut2
	OUT  0x2A+1,R31
	OUT  0x2A,R30
	RJMP _0x2000004
;#pragma used-
;#pragma used+
;void SudutServo7(unsigned char posisi)
; 0000 020A {   dServo7 = SetSudut2(posisi);}
_SudutServo7:
;	posisi -> Y+0
	LD   R30,Y
	LDI  R31,0
	ST   -Y,R31
	ST   -Y,R30
	RCALL _SetSudut2
	OUT  0x28+1,R31
	OUT  0x28,R30
_0x2000004:
	ADIW R28,1
	RET
;#pragma used-
;#pragma used+
;void SudutServo8(unsigned char posisi)
; 0000 020E { unsigned int dServo8;
_SudutServo8:
; 0000 020F   dServo8 = SetSudut2(posisi);
	ST   -Y,R17
	ST   -Y,R16
;	posisi -> Y+2
;	dServo8 -> R16,R17
	LDD  R30,Y+2
	LDI  R31,0
	ST   -Y,R31
	ST   -Y,R30
	RCALL _SetSudut2
	MOVW R16,R30
; 0000 0210   OCR1CH = ByteH(dServo8);
	STS  121,R17
; 0000 0211   OCR1CL = ByteL(dServo8);
	STS  120,R16
; 0000 0212   //TulisdServo8;
; 0000 0213 }
	LDD  R17,Y+1
	LDD  R16,Y+0
_0x2000003:
	ADIW R28,3
	RET
;#pragma used-
;
;
;signed char Error1, Error2;
;#define MaxSpeed (signed int)   400
;#define MaxIntegral             MaxSpeed
;#define KPm (short int) 10
;#define KDm (short int) 3
;#define KIm (short int) 5
;
;#pragma used+
;void PIDmotor2(void)
; 0000 0220 { signed int U;
_PIDmotor2:
; 0000 0221   static signed char lErrorM2=0, iErrorM2=0;
; 0000 0222   // Menghitung PID Motor 2
; 0000 0223   if(dMotor2>0)
	ST   -Y,R17
	ST   -Y,R16
;	U -> R16,R17
	LDS  R26,_dMotor2
	CPI  R26,LOW(0x1)
	BRGE PC+3
	JMP _0x66
; 0000 0224   {   Error2 = (signed char)dMotor2-(signed char)dSpeed2;
	LDS  R26,_dSpeed2
	LDS  R30,_dMotor2
	SUB  R30,R26
	STS  _Error2,R30
; 0000 0225       U = KPm * (Error2);//+lErrorM2);
	LDI  R26,LOW(10)
	MULS R30,R26
	MOVW R16,R0
; 0000 0226       iErrorM2+=(Error2+lErrorM2);
	LDS  R30,_lErrorM2_S0000025000
	LDS  R26,_Error2
	ADD  R30,R26
	LDS  R26,_iErrorM2_S0000025000
	ADD  R30,R26
	STS  _iErrorM2_S0000025000,R30
; 0000 0227       if(iErrorM2>80)      iErrorM2 = 80;
	LDS  R26,_iErrorM2_S0000025000
	CPI  R26,LOW(0x51)
	BRLT _0x67
	LDI  R30,LOW(80)
	RJMP _0xDB
; 0000 0228       else if(iErrorM2<-80)iErrorM2 = -80;
_0x67:
	LDS  R26,_iErrorM2_S0000025000
	CPI  R26,LOW(0xB0)
	BRGE _0x69
	LDI  R30,LOW(176)
_0xDB:
	STS  _iErrorM2_S0000025000,R30
; 0000 0229       U+= KIm * iErrorM2;
_0x69:
	LDS  R30,_iErrorM2_S0000025000
	LDI  R26,LOW(5)
	MULS R30,R26
	MOVW R30,R0
	__ADDWRR 16,17,30,31
; 0000 022A       if (U>MaxSpeed)
	__CPWRN 16,17,401
	BRLT _0x6A
; 0000 022B         {   PwmM2H = ByteH(MaxSpeed);  PwmM2L = ByteL(MaxSpeed);}
	LDI  R30,LOW(1)
	STS  135,R30
	LDI  R30,LOW(144)
	RJMP _0xDC
; 0000 022C       else if (U>0)
_0x6A:
	CLR  R0
	CP   R0,R16
	CPC  R0,R17
	BRGE _0x6C
; 0000 022D         {   PwmM2H = ByteH(U);  PwmM2L = ByteL(U);}
	STS  135,R17
	STS  134,R16
; 0000 022E       else
	RJMP _0x6D
_0x6C:
; 0000 022F         {    PwmM2H = 0; PwmM2L = 0;}
	LDI  R30,LOW(0)
	STS  135,R30
_0xDC:
	STS  134,R30
_0x6D:
; 0000 0230       lErrorM2 = Error2;
	LDS  R30,_Error2
	STS  _lErrorM2_S0000025000,R30
; 0000 0231    }
; 0000 0232   else {PwmM2H = 0; PwmM2L = 0;    iErrorM2=0;}
	RJMP _0x6E
_0x66:
	LDI  R30,LOW(0)
	STS  135,R30
	STS  134,R30
	STS  _iErrorM2_S0000025000,R30
_0x6E:
; 0000 0233 }
	RJMP _0x2000002
;#pragma used-
;
;#pragma used+
;void PIDmotor1(void)
; 0000 0238 { signed int U;
_PIDmotor1:
; 0000 0239   static signed char lErrorM1=0, iErrorM1=0;
; 0000 023A   // Menghitung PID Motor 1
; 0000 023B   if (dMotor1>0)
	ST   -Y,R17
	ST   -Y,R16
;	U -> R16,R17
	LDS  R26,_dMotor1
	CPI  R26,LOW(0x1)
	BRGE PC+3
	JMP _0x6F
; 0000 023C   {   Error1 = (signed char)dMotor1-(signed char)dSpeed1;
	LDS  R26,_dSpeed1
	LDS  R30,_dMotor1
	SUB  R30,R26
	STS  _Error1,R30
; 0000 023D       U = KPm * (lErrorM1);//+Error1);
	LDS  R30,_lErrorM1_S0000026000
	LDI  R26,LOW(10)
	MULS R30,R26
	MOVW R16,R0
; 0000 023E       iErrorM1+=(Error1+lErrorM1);
	LDS  R26,_Error1
	ADD  R30,R26
	LDS  R26,_iErrorM1_S0000026000
	ADD  R30,R26
	STS  _iErrorM1_S0000026000,R30
; 0000 023F       if(iErrorM1>80)      iErrorM1 = 80;
	LDS  R26,_iErrorM1_S0000026000
	CPI  R26,LOW(0x51)
	BRLT _0x70
	LDI  R30,LOW(80)
	RJMP _0xDD
; 0000 0240       else if(iErrorM1<-80)iErrorM1 = -80;
_0x70:
	LDS  R26,_iErrorM1_S0000026000
	CPI  R26,LOW(0xB0)
	BRGE _0x72
	LDI  R30,LOW(176)
_0xDD:
	STS  _iErrorM1_S0000026000,R30
; 0000 0241       U+= KIm * iErrorM1;
_0x72:
	LDS  R30,_iErrorM1_S0000026000
	LDI  R26,LOW(5)
	MULS R30,R26
	MOVW R30,R0
	__ADDWRR 16,17,30,31
; 0000 0242       if (U>MaxSpeed)
	__CPWRN 16,17,401
	BRLT _0x73
; 0000 0243         {   PwmM1H = ByteH(MaxSpeed);  PwmM1L = ByteL(MaxSpeed);}
	LDI  R30,LOW(1)
	STS  133,R30
	LDI  R30,LOW(144)
	RJMP _0xDE
; 0000 0244       else if (U>0)
_0x73:
	CLR  R0
	CP   R0,R16
	CPC  R0,R17
	BRGE _0x75
; 0000 0245         {   PwmM1H = ByteH(U);  PwmM1L = ByteL(U);}
	STS  133,R17
	STS  132,R16
; 0000 0246       else
	RJMP _0x76
_0x75:
; 0000 0247         {    PwmM1H = 0; PwmM1L = 0;}
	LDI  R30,LOW(0)
	STS  133,R30
_0xDE:
	STS  132,R30
_0x76:
; 0000 0248       lErrorM1  = Error1;
	LDS  R30,_Error1
	STS  _lErrorM1_S0000026000,R30
; 0000 0249   }
; 0000 024A   else  {PwmM1H = 0; PwmM1L = 0; iErrorM1=0;}
	RJMP _0x77
_0x6F:
	LDI  R30,LOW(0)
	STS  133,R30
	STS  132,R30
	STS  _iErrorM1_S0000026000,R30
_0x77:
; 0000 024B }
_0x2000002:
	LD   R16,Y+
	LD   R17,Y+
	RET
;#pragma used-
;
;// Interupsi setiap 1 ms
;interrupt [TIM2_COMP] void timer2_comp_isr(void)
; 0000 0250 {   static unsigned char LaguTick=0, _dCounter1=0, _dCounter2=0;
_timer2_comp_isr:
	ST   -Y,R0
	ST   -Y,R1
	ST   -Y,R15
	ST   -Y,R22
	ST   -Y,R23
	ST   -Y,R24
	ST   -Y,R25
	ST   -Y,R26
	ST   -Y,R27
	ST   -Y,R30
	ST   -Y,R31
	IN   R30,RAMPZ
	ST   -Y,R30
	IN   R30,SREG
	ST   -Y,R30
; 0000 0251     static unsigned char TempoTick=0, PIDTickMotor=0;
; 0000 0252     SysTick++;
	INC  R13
; 0000 0253     if(++PIDTickMotor==4)
	LDS  R26,_PIDTickMotor_S0000027000
	SUBI R26,-LOW(1)
	STS  _PIDTickMotor_S0000027000,R26
	CPI  R26,LOW(0x4)
	BRNE _0x78
; 0000 0254     {   if(dCounter1>_dCounter1)    dSpeed1 = dCounter1 - _dCounter1;
	LDS  R30,__dCounter1_S0000027000
	CP   R30,R11
	BRSH _0x79
	LDS  R26,__dCounter1_S0000027000
	MOV  R30,R11
	SUB  R30,R26
	RJMP _0xDF
; 0000 0255         else                        dSpeed1 = 0xFF - _dCounter1 + dCounter1 + 1;
_0x79:
	LDS  R26,__dCounter1_S0000027000
	LDI  R30,LOW(255)
	SUB  R30,R26
	ADD  R30,R11
	SUBI R30,-LOW(1)
_0xDF:
	STS  _dSpeed1,R30
; 0000 0256         _dCounter1 = dCounter1;
	STS  __dCounter1_S0000027000,R11
; 0000 0257     }
; 0000 0258     else if (PIDTickMotor==5)
	RJMP _0x7B
_0x78:
	LDS  R26,_PIDTickMotor_S0000027000
	CPI  R26,LOW(0x5)
	BRNE _0x7C
; 0000 0259     {   if(PIDMotorOn) PIDmotor1();
	SBRC R2,2
	RCALL _PIDmotor1
; 0000 025A     }
; 0000 025B     else if(PIDTickMotor==9)
	RJMP _0x7E
_0x7C:
	LDS  R26,_PIDTickMotor_S0000027000
	CPI  R26,LOW(0x9)
	BRNE _0x7F
; 0000 025C     {   if(dCounter2>_dCounter2)    dSpeed2 = dCounter2 - _dCounter2;
	LDS  R30,__dCounter2_S0000027000
	CP   R30,R10
	BRSH _0x80
	LDS  R26,__dCounter2_S0000027000
	MOV  R30,R10
	SUB  R30,R26
	RJMP _0xE0
; 0000 025D         else                        dSpeed2 = 0xFF - _dCounter2 + dCounter2 + 1;
_0x80:
	LDS  R26,__dCounter2_S0000027000
	LDI  R30,LOW(255)
	SUB  R30,R26
	ADD  R30,R10
	SUBI R30,-LOW(1)
_0xE0:
	STS  _dSpeed2,R30
; 0000 025E         _dCounter2 = dCounter2;
	STS  __dCounter2_S0000027000,R10
; 0000 025F     }
; 0000 0260     else if(PIDTickMotor>=10)
	RJMP _0x82
_0x7F:
	LDS  R26,_PIDTickMotor_S0000027000
	CPI  R26,LOW(0xA)
	BRLO _0x83
; 0000 0261     {   if(PIDMotorOn) PIDmotor2();
	SBRC R2,2
	RCALL _PIDmotor2
; 0000 0262         PIDTickMotor=0;
	LDI  R30,LOW(0)
	STS  _PIDTickMotor_S0000027000,R30
; 0000 0263     }
; 0000 0264 
; 0000 0265     if(LaguOn)
_0x83:
_0x82:
_0x7E:
_0x7B:
	SBRS R2,1
	RJMP _0x85
; 0000 0266     {   if(++TempoTick>=Tempo[LaguTick])
	LDS  R26,_TempoTick_S0000027000
	SUBI R26,-LOW(1)
	STS  _TempoTick_S0000027000,R26
	LDS  R30,_LaguTick_S0000027000
	CLR  R31
	CLR  R22
	CLR  R23
	SUBI R30,LOW(-_Tempo*2)
	SBCI R31,HIGH(-_Tempo*2)
	SBCI R22,BYTE3(-_Tempo*2)
	__GETBRPF 30
	CP   R26,R30
	BRLO _0x86
; 0000 0267         {   TempoTick = 0;
	LDI  R30,LOW(0)
	STS  _TempoTick_S0000027000,R30
; 0000 0268             if (++LaguTick>78) LaguTick=0;
	LDS  R26,_LaguTick_S0000027000
	SUBI R26,-LOW(1)
	STS  _LaguTick_S0000027000,R26
	CPI  R26,LOW(0x4F)
	BRLO _0x87
	STS  _LaguTick_S0000027000,R30
; 0000 0269             FBuzzer(Melodi[LaguTick]);
_0x87:
	LDS  R30,_LaguTick_S0000027000
	LDI  R26,LOW(_Melodi*2)
	LDI  R27,HIGH(_Melodi*2)
	LDI  R24,BYTE3(_Melodi*2)
	CLR  R31
	CLR  R22
	CLR  R23
	CALL __LSLD1
	CALL __ADDD12
	CALL __GETW1PF
	ST   -Y,R31
	ST   -Y,R30
	RCALL _FBuzzer
; 0000 026A         }
; 0000 026B     }
_0x86:
; 0000 026C }
_0x85:
	LD   R30,Y+
	OUT  SREG,R30
	LD   R30,Y+
	OUT  RAMPZ,R30
	LD   R31,Y+
	LD   R30,Y+
	LD   R27,Y+
	LD   R26,Y+
	LD   R25,Y+
	LD   R24,Y+
	LD   R23,Y+
	LD   R22,Y+
	LD   R15,Y+
	LD   R1,Y+
	LD   R0,Y+
	RETI
;
;
;// ------------------- Fungsi Motor ------------------
;#pragma used+
;void SetDataMotor(signed char dmKa, signed char dmKi)
; 0000 0272 {   //dmKa*=2;    dmKi*=2;
_SetDataMotor:
; 0000 0273     if(dmKa<0)  { dMotor2 = -dmKa;  M2_CCW; }
;	dmKa -> Y+1
;	dmKi -> Y+0
	LDD  R26,Y+1
	CPI  R26,0
	BRGE _0x88
	LDD  R30,Y+1
	NEG  R30
	STS  _dMotor2,R30
	CBI  0x3,2
; 0000 0274     else        { dMotor2 = dmKa;   M2_CW;  }
	RJMP _0x8B
_0x88:
	LDD  R30,Y+1
	STS  _dMotor2,R30
	SBI  0x3,2
_0x8B:
; 0000 0275     if(dmKi<0)  { dMotor1 = -dmKi;  M1_CW; }
	LD   R26,Y
	CPI  R26,0
	BRGE _0x8E
	LD   R30,Y
	NEG  R30
	STS  _dMotor1,R30
	CBI  0x3,5
; 0000 0276     else        { dMotor1 = dmKi;   M1_CCW;  }
	RJMP _0x91
_0x8E:
	LD   R30,Y
	STS  _dMotor1,R30
	SBI  0x3,5
_0x91:
; 0000 0277 }
_0x2000001:
	ADIW R28,2
	RET
