;#pragma used+
;void GerakRobot(short int Speed, short int U)
; 0000 02C3 { int PWMKi, PWMKa;
; 0000 02C4    // Menghitung Aktuasi Kendali pada Roda Kanan dan Kiri
; 0000 02C5   PWMKa = Speed + U;
;	Speed -> Y+6
;	U -> Y+4
;	PWMKi -> R16,R17
;	PWMKa -> R18,R19
; 0000 02C6   PWMKi = Speed - U;
; 0000 02C7   // Menghitung Kecepatan dan arah putaran Motor Roda Kanan
; 0000 02C8   if(PWMKa<0)   {PWMKa = -PWMKa; M2_CCW;}
; 0000 02C9   else          {M2_CW;}
; 0000 02CA   if (PWMKa<MaxSpeed)
; 0000 02CB     {   PwmM2H = ByteH(PWMKa);      PwmM2L = ByteL(PWMKa);}
; 0000 02CC   else
; 0000 02CD     {   PwmM2H = ByteH(MaxSpeed);   PwmM2L = ByteL(MaxSpeed);}
; 0000 02CE   // Menghitung Kecepatan dan arah putaran Motor Roda Kiri
; 0000 02CF   if(PWMKi<0)   {PWMKi = -PWMKi; M1_CW;}
; 0000 02D0   else          {M1_CCW;}
; 0000 02D1   if (PWMKi<MaxSpeed)
; 0000 02D2     {   PwmM1H = ByteH(PWMKi);      PwmM1L = ByteL(PWMKi);}
; 0000 02D3   else
; 0000 02D4     {   PwmM1H = ByteH(MaxSpeed);   PwmM1L = ByteL(MaxSpeed);}
; 0000 02D5    //LCD_GotoXY(0,0);    LCD_Angka3(PWMKa);
; 0000 02D6    //LCD_Data(' ');      LCD_Angka3(PWMKi);
; 0000 02D7 }
;#pragma used-
;
;
;#pragma used+
;void SystemInit(void)
; 0000 02DD {
_SystemInit:
; 0000 02DE  //-------------- Inisialisasi Port -----------------------------------
; 0000 02DF     // Port A  = Koneksi ke LCD --> semua bit difungsikan sebagai output
; 0000 02E0     PORTA=0x00; DDRA=0xFF;
	LDI  R30,LOW(0)
	OUT  0x1B,R30
	LDI  R30,LOW(255)
	OUT  0x1A,R30
; 0000 02E1 
; 0000 02E2     // Port B0   = Input Pull-Up (Enkoder1B)
; 0000 02E3     // Port B1   = Input (SCK - ISP programing)
; 0000 02E4     // Port B2   = Input Pull-Up (Enkoder2B)
; 0000 02E5     // Port B3   = Output (Servo 5)
; 0000 02E6     // Port B4   = Output (Buzzer)
; 0000 02E7     // Port B5-7 = Output (Servo 6-8)
; 0000 02E8     PORTB=0x05; DDRB=0xF8;
	LDI  R30,LOW(5)
	OUT  0x18,R30
	LDI  R30,LOW(248)
	OUT  0x17,R30
; 0000 02E9 
; 0000 02EA     // Port C0-3 = Output (LED Active High)
; 0000 02EB     // Port C4-7 = Input Pull-Up (Push Button Active Low)
; 0000 02EC     PORTC=0xFF; DDRC=0xF0;
	LDI  R30,LOW(255)
	OUT  0x15,R30
	LDI  R30,LOW(240)
	OUT  0x14,R30
; 0000 02ED 
; 0000 02EE     // Port D0,1 = Input (I2C)
; 0000 02EF     // Port D2,3 = Input (USART1)
; 0000 02F0     // Port D2,3 = Input (USART1)
; 0000 02F1     // Port D4-7 = Output(Servo1-4)
; 0000 02F2     PORTD=0x00; DDRD=0xF0;
	LDI  R30,LOW(0)
	OUT  0x12,R30
	LDI  R30,LOW(240)
	OUT  0x11,R30
; 0000 02F3 
; 0000 02F4     // Port E0,1 = Input (USART0, ISP programming)
; 0000 02F5     // Port E2   = Output (DirM2)
; 0000 02F6     // Port E3   = Output (PwmM2)
; 0000 02F7     // Port E4   = Output (PwmM1)
; 0000 02F8     // Port E5   = Output (DirM2)
; 0000 02F9     // Port E6,7 = Input Pull-Up (Enkoder1A,2A)
; 0000 02FA     PORTE=0xC0; DDRE=0x3C;
	LDI  R30,LOW(192)
	OUT  0x3,R30
	LDI  R30,LOW(60)
	OUT  0x2,R30
; 0000 02FB 
; 0000 02FC     // Port F  = Input (Input ADC)
; 0000 02FD     PORTF=0x00; DDRF=0x00;
	LDI  R30,LOW(0)
	STS  98,R30
	STS  97,R30
; 0000 02FE 
; 0000 02FF     // Port G  = Input Pull-Up (Saklar Mode Active Low)
; 0000 0300     PORTG=0x17;
	LDI  R30,LOW(23)
	STS  101,R30
; 0000 0301     DDRG=0x00;
	LDI  R30,LOW(0)
	STS  100,R30
; 0000 0302 
; 0000 0303  //-------------- Inisialisasi Timer -----------------------------------
; 0000 0304     // Timer/Counter 0 initialization
; 0000 0305     // Clock source: System Clock
; 0000 0306     // Clock value: 250,000 kHz
; 0000 0307     // Mode: CTC top=OCR0
; 0000 0308     // OC0 output: Toggle on compare match
; 0000 0309     ASSR=0x00;
	OUT  0x30,R30
; 0000 030A     TCCR0=0x00; // sementara dimatikan (diaktifkan ketika fungsi FBuzzer(Frek) dipanggil)
	OUT  0x33,R30
; 0000 030B     TCNT0=0x00;
	OUT  0x32,R30
; 0000 030C     OCR0=0x00;
	OUT  0x31,R30
; 0000 030D 
; 0000 030E     // Timer/Counter 1 initialization
; 0000 030F     // Clock source: System Clock
; 0000 0310     // Clock value: 2000,000 kHz
; 0000 0311     // Mode: Fast PWM top=ICR1
; 0000 0312     // OC1A output: Non-Inv.
; 0000 0313     // OC1B output: Non-Inv.
; 0000 0314     // OC1C output: Non-Inv.
; 0000 0315     // Noise Canceler: Off
; 0000 0316     // Input Capture on Falling Edge
; 0000 0317     // Timer 1 Overflow Interrupt: Off
; 0000 0318     // Input Capture Interrupt: Off
; 0000 0319     // Compare A Match Interrupt: Off
; 0000 031A     // Compare B Match Interrupt: Off
; 0000 031B     // Compare C Match Interrupt: Off
; 0000 031C     TCCR1A=0xAA;
	LDI  R30,LOW(170)
	OUT  0x2F,R30
; 0000 031D     TCCR1B=0x1A;
	LDI  R30,LOW(26)
	OUT  0x2E,R30
; 0000 031E     TCNT1H=0x00;
	LDI  R30,LOW(0)
	OUT  0x2D,R30
; 0000 031F     TCNT1L=0x00;
	OUT  0x2C,R30
; 0000 0320     ICR1H=0x9c; // 40000
	LDI  R30,LOW(156)
	OUT  0x27,R30
; 0000 0321     ICR1L=0x40;
	LDI  R30,LOW(64)
	OUT  0x26,R30
; 0000 0322     OCR1AH=0x00;
	LDI  R30,LOW(0)
	OUT  0x2B,R30
; 0000 0323     OCR1AL=0x00;
	OUT  0x2A,R30
; 0000 0324     OCR1BH=0x00;
	OUT  0x29,R30
; 0000 0325     OCR1BL=0x00;
	OUT  0x28,R30
; 0000 0326     OCR1CH=0x00;
	STS  121,R30
; 0000 0327     OCR1CL=0x00;
	STS  120,R30
; 0000 0328 
; 0000 0329     // Timer/Counter 2 initialization
; 0000 032A     // Clock source: System Clock
; 0000 032B     // Clock value: 62,500 kHz
; 0000 032C     // Mode: CTC top=OCR2
; 0000 032D     // OC2 output: Disconnected
; 0000 032E     //TCCR2=0x0C;       // 64,500 kHz        -- interupsi 4 ms
; 0000 032F     TCCR2=0x0B;         // 250,000 kHz       -- interupsi 1 ms
	LDI  R30,LOW(11)
	OUT  0x25,R30
; 0000 0330     TCNT2=0x00;
	LDI  R30,LOW(0)
	OUT  0x24,R30
; 0000 0331     OCR2=0xFA;
	LDI  R30,LOW(250)
	OUT  0x23,R30
; 0000 0332 
; 0000 0333     // Timer/Counter 3 initialization
; 0000 0334     // Clock source: System Clock
; 0000 0335     // Clock value: 16000,000 kHz
; 0000 0336     // Mode: Ph. correct PWM top=ICR3
; 0000 0337     // Noise Canceler: Off
; 0000 0338     // Input Capture on Falling Edge
; 0000 0339     // OC3A output: Inverted
; 0000 033A     // OC3B output: Inverted
; 0000 033B     // OC3C output: Discon.
; 0000 033C     // Timer 3 Overflow Interrupt: Off
; 0000 033D     // Input Capture Interrupt: Off
; 0000 033E     // Compare A Match Interrupt: Off
; 0000 033F     // Compare B Match Interrupt: Off
; 0000 0340     // Compare C Match Interrupt: On
; 0000 0341     TCCR3A=0xF2;
	LDI  R30,LOW(242)
	STS  139,R30
; 0000 0342     TCCR3B=0x11;
	LDI  R30,LOW(17)
	STS  138,R30
; 0000 0343     TCNT3H=0x00;
	LDI  R30,LOW(0)
	STS  137,R30
; 0000 0344     TCNT3L=0x00;
	STS  136,R30
; 0000 0345     ICR3H=0x01;  // 400
	LDI  R30,LOW(1)
	STS  129,R30
; 0000 0346     ICR3L=0x90;
	LDI  R30,LOW(144)
	STS  128,R30
; 0000 0347     OCR3AH=0x00;
	LDI  R30,LOW(0)
	STS  135,R30
; 0000 0348     OCR3AL=0x00;
	STS  134,R30
; 0000 0349     OCR3BH=0x00;
	STS  133,R30
; 0000 034A     OCR3BL=0x00;
	STS  132,R30
; 0000 034B     OCR3CH=0x00;
	STS  131,R30
; 0000 034C     OCR3CL=0xC8; // 200
	LDI  R30,LOW(200)
	STS  130,R30
; 0000 034D 
; 0000 034E     // External Interrupt(s) initialization
; 0000 034F // INT0: Off
; 0000 0350 // INT1: Off
; 0000 0351 // INT2: Off
; 0000 0352 // INT3: Off
; 0000 0353 // INT4: Off
; 0000 0354 // INT5: Off
; 0000 0355 // INT6: On
; 0000 0356 // INT6 Mode: Falling Edge
; 0000 0357 // INT7: On
; 0000 0358 // INT7 Mode: Falling Edge
; 0000 0359     EICRA=0x00;
	LDI  R30,LOW(0)
	STS  106,R30
; 0000 035A     EICRB=0xA0;
	LDI  R30,LOW(160)
	OUT  0x3A,R30
; 0000 035B     EIMSK=0xC0;
	LDI  R30,LOW(192)
	OUT  0x39,R30
; 0000 035C     EIFR=0xC0;
	OUT  0x38,R30
; 0000 035D 
; 0000 035E // External Interrupt(s) initialization
; 0000 035F // INT0: Off
; 0000 0360 // INT1: Off
; 0000 0361 // INT2: Off
; 0000 0362 // INT3: Off
; 0000 0363 // INT4: Off
; 0000 0364 // INT5: Off
; 0000 0365 // INT6: On
; 0000 0366 // INT6 Mode: Any change
; 0000 0367 // INT7: On
; 0000 0368 // INT7 Mode: Any change
; 0000 0369     EICRA=0x00;
	LDI  R30,LOW(0)
	STS  106,R30
; 0000 036A     EICRB=0x50;
	LDI  R30,LOW(80)
	OUT  0x3A,R30
; 0000 036B     EIMSK=0xC0;
	LDI  R30,LOW(192)
	OUT  0x39,R30
; 0000 036C     EIFR=0xC0;
	OUT  0x38,R30
; 0000 036D 
; 0000 036E     // Timer(s)/Counter(s) Interrupt(s) initialization
; 0000 036F     TIMSK=0x80;
	LDI  R30,LOW(128)
	OUT  0x37,R30
; 0000 0370     ETIMSK=0x02;
	LDI  R30,LOW(2)
	STS  125,R30
; 0000 0371 
; 0000 0372 // ------------------------ Inisialisasi USART1 ----------------------
; 0000 0373     // Communication Parameters: 8 Data, 1 Stop, No Parity
; 0000 0374     // USART1 Receiver: On
; 0000 0375     // USART1 Transmitter: On
; 0000 0376     // USART1 Mode: Asynchronous
; 0000 0377     // USART1 Baud rate: 9600
; 0000 0378     UCSR1A=0x00;
	LDI  R30,LOW(0)
	STS  155,R30
; 0000 0379     UCSR1B=0x98;
	LDI  R30,LOW(152)
	STS  154,R30
; 0000 037A     UCSR1C=0x06;
	LDI  R30,LOW(6)
	STS  157,R30
; 0000 037B     UBRR1H=0x00;
	LDI  R30,LOW(0)
	STS  152,R30
; 0000 037C     UBRR1L=0x67;
	LDI  R30,LOW(103)
	STS  153,R30
; 0000 037D 
; 0000 037E // ------------  Inisialisasi Analog Comparator -----------------------
; 0000 037F     // Analog Comparator: Off
; 0000 0380     // Analog Comparator Input Capture by Timer/Counter 1: Off
; 0000 0381     ACSR=0x80;
	LDI  R30,LOW(128)
	OUT  0x8,R30
; 0000 0382     SFIOR=0x00;
	LDI  R30,LOW(0)
	OUT  0x20,R30
; 0000 0383 
; 0000 0384 // ------------ Inisialisasi Peripheral Board ------------------------
; 0000 0385 
; 0000 0386     LCD_Init(); // -------------------------------------- LCD Text 16x2
	RCALL _LCD_Init
; 0000 0387 
; 0000 0388     SudutServo1(0); // -------------------------------Reset Sudut Servo
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo1
; 0000 0389     SudutServo2(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo2
; 0000 038A     SudutServo3(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo3
; 0000 038B     SudutServo4(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo4
; 0000 038C     SudutServo5(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo5
; 0000 038D     SudutServo6(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo6
; 0000 038E     SudutServo7(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo7
; 0000 038F     SudutServo8(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo8
; 0000 0390 
; 0000 0391 // -------------- Global enable interrupts ----------------------------
; 0000 0392     #asm("sei")
	sei
; 0000 0393 }
	RET
;#pragma used-
;
;
;void main(void)
; 0000 0398 {   unsigned int i;
_main:
; 0000 0399     SystemInit();
;	i -> R16,R17
	RCALL _SystemInit
; 0000 039A     //dServo1 = 20;
; 0000 039B     //dServo2 = 80;
; 0000 039C     Stop();
	RCALL _Stop
; 0000 039D     SudutServo1(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo1
; 0000 039E     SudutServo2(45);
	LDI  R30,LOW(45)
	ST   -Y,R30
	RCALL _SudutServo2
; 0000 039F     SudutServo3(90);
	LDI  R30,LOW(90)
	ST   -Y,R30
	RCALL _SudutServo3
; 0000 03A0     SudutServo4(135);
	LDI  R30,LOW(135)
	ST   -Y,R30
	RCALL _SudutServo4
; 0000 03A1     SudutServo5(180);
	LDI  R30,LOW(180)
	ST   -Y,R30
	RCALL _SudutServo5
; 0000 03A2     SudutServo6(180);
	LDI  R30,LOW(180)
	ST   -Y,R30
	RCALL _SudutServo6
; 0000 03A3     SudutServo7(0);
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _SudutServo7
; 0000 03A4     SudutServo8(90);
	LDI  R30,LOW(90)
	ST   -Y,R30
	RCALL _SudutServo8
; 0000 03A5     LCD_GotoXY(0,0);    LCD_Angka3(dServo1);
