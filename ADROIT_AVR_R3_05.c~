	RCALL _SetDataMotor
; 0000 03B3 
; 0000 03B4     while (1)
_0xB4:
; 0000 03B5       {
; 0000 03B6       // Place your code here
; 0000 03B7 
; 0000 03B8          LCD_BackLight = 1;
	SET
	BLD  R2,0
; 0000 03B9          //LCD_Tulis1("1234567890123456");
; 0000 03BA          //LCD_Tulis2("1234567890123456");
; 0000 03BB          //LED1 = !PB1 ^ MODE1;
; 0000 03BC          LED2 = !PB2 ^ MODE2;
	LDI  R26,0
	SBIS 0x13,2
	LDI  R26,1
	LDS  R30,99
	ANDI R30,LOW(0x4)
	BRNE _0xB7
	LDI  R30,LOW(0)
	RJMP _0xB8
_0xB7:
	LDI  R30,LOW(1)
_0xB8:
	EOR  R30,R26
	BRNE _0xBA
	CBI  0x15,5
	RJMP _0xBB
_0xBA:
	SBI  0x15,5
_0xBB:
; 0000 03BD          LED3 = !PB3 ^ MODE3;
