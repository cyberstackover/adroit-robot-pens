	ST   -Y,R30
	ST   -Y,R30
	RCALL _LCD_GotoXY
	LDS  R30,_dServo1
	LDI  R31,0
	ST   -Y,R31
	ST   -Y,R30
	RCALL _LCD_Angka3
; 0000 03A6     LCD_GotoXY(4,0);    LCD_Angka3(dServo2);
