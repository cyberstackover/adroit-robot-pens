;// USART1 Receiver buffer
;#define RX_BUFFER_SIZE1 8
;char rx_buffer1[RX_BUFFER_SIZE1];
;
;#if RX_BUFFER_SIZE1<256
;unsigned char rx_wr_index1,rx_rd_index1,rx_counter1;
;#else
;unsigned int rx_wr_index1,rx_rd_index1,rx_counter1;
;#endif
;
;// This flag is set on USART1 Receiver buffer overflow
;bit rx_buffer_overflow1;
;
;// USART1 Receiver interrupt service routine
;interrupt [USART1_RXC] void usart1_rx_isr(void)
; 0000 00D8 {

	.CSEG
_usart1_rx_isr:
	ST   -Y,R26
	ST   -Y,R30
	ST   -Y,R31
	IN   R30,SREG
	ST   -Y,R30
; 0000 00D9 char status,data;
; 0000 00DA 
; 0000 00DB status=UCSR1A;
	ST   -Y,R17
	ST   -Y,R16
;	status -> R17
;	data -> R16
	LDS  R17,155
; 0000 00DC data=UDR1;
	LDS  R16,156
; 0000 00DD if ((status & (FRAMING_ERROR | PARITY_ERROR | DATA_OVERRUN))==0)
	MOV  R30,R17
	ANDI R30,LOW(0x1C)
	BRNE _0x3
; 0000 00DE    {
; 0000 00DF    rx_buffer1[rx_wr_index1]=data;
	LDS  R30,_rx_wr_index1
	LDI  R31,0
	SUBI R30,LOW(-_rx_buffer1)
	SBCI R31,HIGH(-_rx_buffer1)
	ST   Z,R16
; 0000 00E0    if (++rx_wr_index1 == RX_BUFFER_SIZE1) rx_wr_index1=0;
	LDS  R26,_rx_wr_index1
	SUBI R26,-LOW(1)
	STS  _rx_wr_index1,R26
	CPI  R26,LOW(0x8)
	BRNE _0x4
	LDI  R30,LOW(0)
	STS  _rx_wr_index1,R30
; 0000 00E1    if (++rx_counter1 == RX_BUFFER_SIZE1)
_0x4:
	LDS  R26,_rx_counter1
	SUBI R26,-LOW(1)
	STS  _rx_counter1,R26
	CPI  R26,LOW(0x8)
	BRNE _0x5
; 0000 00E2       {
; 0000 00E3       rx_counter1=0;
	LDI  R30,LOW(0)
	STS  _rx_counter1,R30
; 0000 00E4       rx_buffer_overflow1=1;
	SET
	BLD  R2,3
; 0000 00E5       };
_0x5:
; 0000 00E6    };
_0x3:
; 0000 00E7 }
	LD   R16,Y+
	LD   R17,Y+
	LD   R30,Y+
	OUT  SREG,R30
	LD   R31,Y+
	LD   R30,Y+
	LD   R26,Y+
	RETI
;
;// Get a character from the USART1 Receiver buffer
;#pragma used+
;char getchar1(void)
; 0000 00EC {
; 0000 00ED char data;
; 0000 00EE while (rx_counter1==0);
;	data -> R17
; 0000 00EF data=rx_buffer1[rx_rd_index1];
; 0000 00F0 if (++rx_rd_index1 == RX_BUFFER_SIZE1) rx_rd_index1=0;
; 0000 00F1 #asm("cli")
; 0000 00F2 --rx_counter1;
; 0000 00F3 #asm("sei")
; 0000 00F4 return data;
; 0000 00F5 }
;#pragma used-
;// Write a character to the USART1 Transmitter
;#pragma used+
;void putchar1(char c)
; 0000 00FA {
; 0000 00FB while ((UCSR1A & DATA_REGISTER_EMPTY)==0);
;	c -> Y+0
; 0000 00FC UDR1=c;
; 0000 00FD }
;#pragma used-
;
;
;// Rotari Enkoder Motor 1
;interrupt [EXT_INT6] void ext_int6_isr(void)
; 0000 0103 { if(P_Enkoder1A)
_ext_int6_isr:
	ST   -Y,R30
	ST   -Y,R31
	IN   R30,SREG
	ST   -Y,R30
	SBIS 0x1,6
	RJMP _0xD
; 0000 0104   {  if(P_Enkoder1B) {#asm("cli")  Enkoder1--; #asm("sei")}
	SBIS 0x16,0
	RJMP _0xE
	cli
	MOVW R30,R6
	SBIW R30,1
	MOVW R6,R30
	sei
; 0000 0105      else            {#asm("cli")  Enkoder1++; #asm("sei")}
	RJMP _0xF
_0xE:
	cli
	MOVW R30,R6
	ADIW R30,1
	MOVW R6,R30
	sei
_0xF:
; 0000 0106   }
; 0000 0107   else
	RJMP _0x10
_0xD:
; 0000 0108   {  if(P_Enkoder1B) {#asm("cli")  Enkoder1++; #asm("sei")}
	SBIS 0x16,0
	RJMP _0x11
	cli
	MOVW R30,R6
	ADIW R30,1
	MOVW R6,R30
	sei
; 0000 0109      else            {#asm("cli")  Enkoder1--; #asm("sei")}
	RJMP _0x12
_0x11:
	cli
	MOVW R30,R6
	SBIW R30,1
	MOVW R6,R30
	sei
_0x12:
; 0000 010A   }
_0x10:
; 0000 010B   dCounter1++;
	INC  R11
; 0000 010C }
	LD   R30,Y+
	OUT  SREG,R30
	LD   R31,Y+
	LD   R30,Y+
	RETI
;
;// Rotari Enkoder Motor 2
;interrupt [EXT_INT7] void ext_int7_isr(void)
; 0000 0110 { if(P_Enkoder2A)
_ext_int7_isr:
	ST   -Y,R30
	ST   -Y,R31
	IN   R30,SREG
	ST   -Y,R30
	SBIS 0x1,7
	RJMP _0x13
; 0000 0111   {  if(P_Enkoder2B) {#asm("cli")  Enkoder2--; #asm("sei")}
	SBIS 0x16,2
	RJMP _0x14
	cli
	MOVW R30,R8
	SBIW R30,1
	MOVW R8,R30
	sei
; 0000 0112      else            {#asm("cli")  Enkoder2++; #asm("sei")}
	RJMP _0x15
_0x14:
	cli
	MOVW R30,R8
	ADIW R30,1
	MOVW R8,R30
	sei
_0x15:
; 0000 0113   }
; 0000 0114   else
	RJMP _0x16
_0x13:
; 0000 0115   {  if(P_Enkoder2B) {#asm("cli")  Enkoder2++; #asm("sei")}
	SBIS 0x16,2
	RJMP _0x17
	cli
	MOVW R30,R8
	ADIW R30,1
	MOVW R8,R30
	sei
; 0000 0116      else            {#asm("cli")  Enkoder2--; #asm("sei")}
	RJMP _0x18
_0x17:
	cli
	MOVW R30,R8
	SBIW R30,1
	MOVW R8,R30
	sei
_0x18:
; 0000 0117   }
_0x16:
; 0000 0118   dCounter2++;
	INC  R10
; 0000 0119 }
	LD   R30,Y+
	OUT  SREG,R30
	LD   R31,Y+
	LD   R30,Y+
	RETI
;
;
;// Interupsi dengan frekwensi 40KHz -- Timer 3 output compare C interrupt service routine
;interrupt [TIM3_COMPC] void timer3_compc_isr(void)
; 0000 011E {  if (++ServoCounter<=80)
_timer3_compc_isr:
	ST   -Y,R26
	ST   -Y,R30
	ST   -Y,R31
	IN   R30,SREG
	ST   -Y,R30
	MOVW R30,R4
	ADIW R30,1
	MOVW R4,R30
	CPI  R30,LOW(0x51)
	LDI  R26,HIGH(0x51)
	CPC  R31,R26
	BRSH _0x19
; 0000 011F    {    if ((unsigned char)ServoCounter==dServo1) {P_Servo1=0;}
	LDS  R30,_dServo1
	CP   R30,R4
	BRNE _0x1A
	CBI  0x12,4
; 0000 0120         if ((unsigned char)ServoCounter==dServo2) {P_Servo2=0;}
_0x1A:
	LDS  R30,_dServo2
	CP   R30,R4
	BRNE _0x1D
	CBI  0x12,5
; 0000 0121         if ((unsigned char)ServoCounter==dServo3) {P_Servo3=0;}
_0x1D:
	LDS  R30,_dServo3
	CP   R30,R4
	BRNE _0x20
	CBI  0x12,6
; 0000 0122         if ((unsigned char)ServoCounter==dServo4) {P_Servo4=0;}
_0x20:
	LDS  R30,_dServo4
	CP   R30,R4
	BRNE _0x23
	CBI  0x12,7
; 0000 0123         if ((unsigned char)ServoCounter==dServo5) {P_Servo5=0;}
_0x23:
	CP   R12,R4
	BRNE _0x26
	CBI  0x18,3
; 0000 0124    }
_0x26:
; 0000 0125    else if (ServoCounter>800)
	RJMP _0x29
_0x19:
	LDI  R30,LOW(800)
	LDI  R31,HIGH(800)
	CP   R30,R4
	CPC  R31,R5
	BRSH _0x2A
; 0000 0126    {    ServoCounter = 0;
	CLR  R4
	CLR  R5
; 0000 0127         P_Servo1_4T = P_Servo1_4B | 0xF0; // P_Servo1 = 1;    P_Servo2 = 1;    P_Servo3 = 1;    P_Servo4 = 1;
	IN   R30,0x10
	ORI  R30,LOW(0xF0)
	OUT  0x12,R30
; 0000 0128         P_Servo5 = 1;
	SBI  0x18,3
; 0000 0129    }
; 0000 012A }
_0x2A:
_0x29:
	LD   R30,Y+
	OUT  SREG,R30
	LD   R31,Y+
	LD   R30,Y+
	LD   R26,Y+
	RETI
;
;// Fungsi LCD
;#pragma used+
;void LCD_Init_Cmd(unsigned char xData)
; 0000 012F {   LCD_PORT =  0B00000100 | (xData & 0xF0);
_LCD_Init_Cmd:
;	xData -> Y+0
	LD   R30,Y
	ANDI R30,LOW(0xF0)
	ORI  R30,4
	OUT  0x1B,R30
; 0000 0130     delay_us(1);        LCD_EN = 0;
	__DELAY_USB 5
	CBI  0x1B,2
; 0000 0131 }
	RJMP _0x2000004
;void LCD_Perintah(unsigned char xData)
; 0000 0133 {   //LCD_RS=0;   LCD_RW=0;   LCD_EN=1;
_LCD_Perintah:
; 0000 0134     LCD_RS=0;
;	xData -> Y+0
	CBI  0x1B,0
; 0000 0135     LCD_PORT =  0B00001100 | (xData & 0xF0);
	LD   R30,Y
	ANDI R30,LOW(0xF0)
	ORI  R30,LOW(0xC)
	OUT  0x1B,R30
; 0000 0136     delay_us(1);     LCD_EN = 0;
	__DELAY_USB 5
	CBI  0x1B,2
; 0000 0137     delay_us(5);
	__DELAY_USB 27
; 0000 0138     LCD_PORT =  0B00001100 | (xData << 4);
	LD   R30,Y
	SWAP R30
	ANDI R30,0xF0
	ORI  R30,LOW(0xC)
	OUT  0x1B,R30
; 0000 0139     delay_us(1);     LCD_EN = 0;
	__DELAY_USB 5
	CBI  0x1B,2
; 0000 013A     delay_us(700);
	__DELAY_USW 2800
; 0000 013B     LCD_EN = 1;
	SBI  0x1B,2
; 0000 013C }
	RJMP _0x2000004
;#pragma used-
;#pragma used+
;void LCD_Data(unsigned char xData)
; 0000 0140 {   //LCD_RS=1;   LCD_RW=0;   LCD_EN=1;
_LCD_Data:
; 0000 0141     LCD_RS=1;
;	xData -> Y+0
	SBI  0x1B,0
; 0000 0142     LCD_PORT =  0B00001101 | (xData & 0xF0);
	LD   R30,Y
	ANDI R30,LOW(0xF0)
	ORI  R30,LOW(0xD)
	OUT  0x1B,R30
; 0000 0143     delay_us(1);     LCD_EN = 0;
	__DELAY_USB 5
	CBI  0x1B,2
; 0000 0144     delay_us(5);
	__DELAY_USB 27
; 0000 0145     LCD_PORT =  0B00001101 | (xData << 4);
	LD   R30,Y
	SWAP R30
	ANDI R30,0xF0
	ORI  R30,LOW(0xD)
	OUT  0x1B,R30
; 0000 0146     delay_us(1);     LCD_EN = 0;
	__DELAY_USB 5
	CBI  0x1B,2
; 0000 0147     delay_us(40);
	__DELAY_USB 213
; 0000 0148     LCD_EN = 1;
	SBI  0x1B,2
; 0000 0149 }
	RJMP _0x2000004
;#pragma used-
;#pragma used+
;void LCD_Init(void)
; 0000 014D {   delay_ms(50);   LCD_Init_Cmd(0x30);
_LCD_Init:
	LDI  R30,LOW(50)
	LDI  R31,HIGH(50)
	ST   -Y,R31
	ST   -Y,R30
	CALL _delay_ms
	LDI  R30,LOW(48)
	ST   -Y,R30
	RCALL _LCD_Init_Cmd
; 0000 014E     delay_ms(5);    LCD_Init_Cmd(0x30);
	LDI  R30,LOW(5)
	LDI  R31,HIGH(5)
	ST   -Y,R31
	ST   -Y,R30
	CALL _delay_ms
	LDI  R30,LOW(48)
	ST   -Y,R30
	RCALL _LCD_Init_Cmd
; 0000 014F     delay_ms(1);    LCD_Init_Cmd(0x30);
	LDI  R30,LOW(1)
	LDI  R31,HIGH(1)
	ST   -Y,R31
	ST   -Y,R30
	CALL _delay_ms
	LDI  R30,LOW(48)
	ST   -Y,R30
	RCALL _LCD_Init_Cmd
; 0000 0150     delay_ms(1);    LCD_Init_Cmd(0x20);
	LDI  R30,LOW(1)
	LDI  R31,HIGH(1)
	ST   -Y,R31
	ST   -Y,R30
	CALL _delay_ms
	LDI  R30,LOW(32)
	ST   -Y,R30
	RCALL _LCD_Init_Cmd
; 0000 0151     LCD_Perintah(0x28);         //4-bit/2-line
	LDI  R30,LOW(40)
	ST   -Y,R30
	RCALL _LCD_Perintah
; 0000 0152     LCD_Perintah(0x10);         // Set cursor
	LDI  R30,LOW(16)
	ST   -Y,R30
	RCALL _LCD_Perintah
; 0000 0153     LCD_Perintah(0x0c);         // Display ON; Cursor off
	LDI  R30,LOW(12)
	ST   -Y,R30
	RCALL _LCD_Perintah
; 0000 0154     LCD_Perintah(0x06);         // Entry mode =increment, no shift
	LDI  R30,LOW(6)
	ST   -Y,R30
	RCALL _LCD_Perintah
; 0000 0155     LCD_Perintah(0x01);         // Hapus layar
	LDI  R30,LOW(1)
	ST   -Y,R30
	RCALL _LCD_Perintah
; 0000 0156 }
	RET
;#pragma used-
;#pragma used+
;void LCD_GotoXY(unsigned char x, unsigned char y)
; 0000 015A {   unsigned char baris;
_LCD_GotoXY:
; 0000 015B     if (y==0) LCD_Perintah(0x80 + x);          // baris 1 --> y=0
	ST   -Y,R17
;	x -> Y+2
;	y -> Y+1
;	baris -> R17
	LDD  R30,Y+1
	CPI  R30,0
	BRNE _0x3F
	LDD  R30,Y+2
	SUBI R30,-LOW(128)
	RJMP _0xD9
; 0000 015C     else      LCD_Perintah(0x80 + x + 0x40);   // baris 2 --> y=1
_0x3F:
	LDD  R30,Y+2
	SUBI R30,-LOW(192)
_0xD9:
	ST   -Y,R30
	RCALL _LCD_Perintah
; 0000 015D }
	LDD  R17,Y+0
	RJMP _0x2000003
;#pragma used-
;#pragma used+
;void LCD_Tulis(unsigned char flash *text)        // menuliskan string ke LCD
; 0000 0161 {   while (*text!=0)
;	*text -> Y+0
; 0000 0162     { LCD_Data(*text);text++;}
; 0000 0163 }
;#pragma used-
;#pragma used+
;void LCD_Tulis1(unsigned char flash *text)        // menuliskan string ke LCD
; 0000 0167 {   LCD_GotoXY(0,0);LCD_Tulis(text);
;	*text -> Y+0
; 0000 0168 }
;#pragma used-
;#pragma used+
;void LCD_Tulis2(unsigned char flash *text)        // menuliskan string ke LCD
; 0000 016C {   LCD_GotoXY(0,1);LCD_Tulis(text);
;	*text -> Y+0
; 0000 016D }
;#pragma used-
;#pragma used+
;void LCD_Hapus(void)              // menghapus seluruh layar
; 0000 0171 {   LCD_Perintah(0x01);
; 0000 0172 }
;#pragma used-
;#pragma used+
;void LCD_Hapus1(void)             // menghapus baris pertama
; 0000 0176 {   unsigned char i;
; 0000 0177     LCD_GotoXY(0,0);
;	i -> R17
; 0000 0178     for(i=0;i<16;i++) LCD_Data(' ');
; 0000 0179 }
;#pragma used-
;#pragma used+
;void LCD_Hapus2(void)             // menghapus baris ke dua
; 0000 017D {   unsigned char i;
; 0000 017E     LCD_GotoXY(0,1);
;	i -> R17
; 0000 017F     for(i=0;i<16;i++) LCD_Data(' ');
; 0000 0180 }
;#pragma used-
;#pragma used+
;void LCD_TulisKiri1(unsigned char flash *text)    // menuliskan string dari arah kiri dgn delay pada baris 1
; 0000 0184 {   unsigned char i;
; 0000 0185     LCD_GotoXY(0,0);
;	*text -> Y+1
;	i -> R17
; 0000 0186     for(i=0;i<16;i++)
; 0000 0187     { LCD_Data(*(text+i)); delay_ms(100);}
; 0000 0188 }
;#pragma used-
;#pragma used+
;void LCD_TulisKiri2(unsigned char flash *text)    // menuliskan string dari arah kiri dgn delay pada baris 2
; 0000 018C {   unsigned char i;
; 0000 018D     LCD_GotoXY(0,1);
;	*text -> Y+1
;	i -> R17
; 0000 018E     for(i=0;i<16;i++)
; 0000 018F     { LCD_Data(*(text+i)); delay_ms(100);}
; 0000 0190 }
;#pragma used-
;#pragma used+
;void LCD_TulisKanan1(unsigned char flash *text)    // menuliskan string dari arah kanan dgn delay pada baris 1
; 0000 0194 {   signed char i;
; 0000 0195     LCD_GotoXY(0,0);
;	*text -> Y+1
;	i -> R17
; 0000 0196     for(i=15;i>-1;i--)
; 0000 0197     { LCD_Perintah (0x80+i);   LCD_Data(*(text+i)); delay_ms(100);}
; 0000 0198 }
;#pragma used-
;#pragma used+
;void LCD_TulisKanan2(unsigned char flash *text)    // menuliskan string dari arah kanan dgn delay pada baris 2
; 0000 019C {   signed char i;
; 0000 019D     LCD_GotoXY(0,1);
;	*text -> Y+1
;	i -> R17
; 0000 019E     for(i=15;i>-1;i--)
; 0000 019F     { LCD_Perintah (0xC0+i);   LCD_Data(*(text+i)); delay_ms(100);}
; 0000 01A0 }
;#pragma used-
;#pragma used+
;void LCD_Angka4(int x)
; 0000 01A4 {   if(x<0){ x*=-1;  LCD_Data('-');}
_LCD_Angka4:
;	x -> Y+0
	LDD  R26,Y+1
	TST  R26
	BRPL _0x56
	LD   R30,Y
	LDD  R31,Y+1
	LDI  R26,LOW(65535)
	LDI  R27,HIGH(65535)
	CALL __MULW12
	ST   Y,R30
	STD  Y+1,R31
	LDI  R30,LOW(45)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01A5     LCD_Data(x/1000+0x30);          // menulis ribuan
_0x56:
	LD   R26,Y
	LDD  R27,Y+1
	LDI  R30,LOW(1000)
	LDI  R31,HIGH(1000)
	CALL __DIVW21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01A6     LCD_Data((x%1000)/100+0x30);    // menulis ratusan
	LD   R26,Y
	LDD  R27,Y+1
	LDI  R30,LOW(1000)
	LDI  R31,HIGH(1000)
	CALL __MODW21
	MOVW R26,R30
	LDI  R30,LOW(100)
	LDI  R31,HIGH(100)
	CALL __DIVW21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01A7     LCD_Data((x%100)/10+0x30);      // menulis puluhan
	LD   R26,Y
	LDD  R27,Y+1
	LDI  R30,LOW(100)
	LDI  R31,HIGH(100)
	CALL __MODW21
	MOVW R26,R30
	LDI  R30,LOW(10)
	LDI  R31,HIGH(10)
	CALL __DIVW21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01A8     LCD_Data(x%10+0x30);            // menulis satuan
	LD   R26,Y
	LDD  R27,Y+1
	LDI  R30,LOW(10)
	LDI  R31,HIGH(10)
	CALL __MODW21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01A9 }
	RJMP _0x2000001
;#pragma used-
;#pragma used+
;void LCD_Angka3(int x)
; 0000 01AD {   if(x<0){ x*=-1;  LCD_Data('-');}
_LCD_Angka3:
;	x -> Y+0
	LDD  R26,Y+1
	TST  R26
	BRPL _0x57
	LD   R30,Y
	LDD  R31,Y+1
	LDI  R26,LOW(65535)
	LDI  R27,HIGH(65535)
	CALL __MULW12
	ST   Y,R30
	STD  Y+1,R31
	LDI  R30,LOW(45)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01AE     LCD_Data(x/100+0x30);           // menulis ratusan
_0x57:
	LD   R26,Y
	LDD  R27,Y+1
	LDI  R30,LOW(100)
	LDI  R31,HIGH(100)
	CALL __DIVW21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01AF     LCD_Data((x%100)/10+0x30);      // menulis puluhan
	LD   R26,Y
	LDD  R27,Y+1
	LDI  R30,LOW(100)
	LDI  R31,HIGH(100)
	CALL __MODW21
	MOVW R26,R30
	LDI  R30,LOW(10)
	LDI  R31,HIGH(10)
	CALL __DIVW21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01B0     LCD_Data(x%10+0x30);            // menulis satuan
	LD   R26,Y
	LDD  R27,Y+1
	LDI  R30,LOW(10)
	LDI  R31,HIGH(10)
	CALL __MODW21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01B1 }
	RJMP _0x2000001
;#pragma used-
;#pragma used+
;void LCD_sByte(signed char x)
; 0000 01B5 {   if(x<0){ x*=-1;  LCD_Data('-');}
_LCD_sByte:
;	x -> Y+0
	LD   R26,Y
	CPI  R26,0
	BRGE _0x58
	LD   R30,Y
	LDI  R26,LOW(255)
	MULS R30,R26
	MOVW R30,R0
	ST   Y,R30
	LDI  R30,LOW(45)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01B6     LCD_Data(x/100+0x30);           // menulis ratusan
_0x58:
	LD   R26,Y
	LDI  R30,LOW(100)
	CALL __DIVB21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01B7     LCD_Data((x%100)/10+0x30);      // menulis puluhan
	LD   R26,Y
	LDI  R30,LOW(100)
	CALL __MODB21
	MOV  R26,R30
	LDI  R30,LOW(10)
	CALL __DIVB21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01B8     LCD_Data(x%10+0x30);            // menulis satuan
	LD   R26,Y
	LDI  R30,LOW(10)
	CALL __MODB21
	SUBI R30,-LOW(48)
	ST   -Y,R30
	RCALL _LCD_Data
; 0000 01B9 }
	RJMP _0x2000004
;#pragma used-
;
;// -----------------  Fungsi Buzzer ----------------------------
;#pragma used+
;void BuzzerOff()
; 0000 01BF { TCCR0=0x00; PORTB.4=0; ASSR=0x00; }
_BuzzerOff:
	LDI  R30,LOW(0)
	OUT  0x33,R30
	CBI  0x18,4
	OUT  0x30,R30
	RET
;#pragma used-
;#pragma used+
;void FBuzzer(unsigned int x)
; 0000 01C3 {
_FBuzzer:
; 0000 01C4   if(x>=125 && x<=10000)
;	x -> Y+0
	LD   R26,Y
	LDD  R27,Y+1
	CPI  R26,LOW(0x7D)
	LDI  R30,HIGH(0x7D)
	CPC  R27,R30
	BRLO _0x5C
	CPI  R26,LOW(0x2711)
	LDI  R30,HIGH(0x2711)
	CPC  R27,R30
	BRLO _0x5D
_0x5C:
	RJMP _0x5B
_0x5D:
; 0000 01C5   {     if (x>=2000)
	LD   R26,Y
	LDD  R27,Y+1
	CPI  R26,LOW(0x7D0)
	LDI  R30,HIGH(0x7D0)
	CPC  R27,R30
	BRLO _0x5E
; 0000 01C6         { TCCR0=0x1A; TCNT0=0;
	LDI  R30,LOW(26)
	OUT  0x33,R30
	LDI  R30,LOW(0)
	OUT  0x32,R30
; 0000 01C7           OCR0 = 500000 / x;
	LD   R30,Y
	LDD  R31,Y+1
	CLR  R22
	CLR  R23
	__GETD2N 0x7A120
	CALL __DIVD21
	OUT  0x31,R30
; 0000 01C8         }
; 0000 01C9         if (x>=1000)
_0x5E:
	LD   R26,Y
	LDD  R27,Y+1
	CPI  R26,LOW(0x3E8)
	LDI  R30,HIGH(0x3E8)
	CPC  R27,R30
	BRLO _0x5F
; 0000 01CA         { TCCR0=0x1B; TCNT0=0;
	LDI  R30,LOW(27)
	OUT  0x33,R30
	LDI  R30,LOW(0)
	OUT  0x32,R30
; 0000 01CB           OCR0 = 250000 / x;
	LD   R30,Y
	LDD  R31,Y+1
	CLR  R22
	CLR  R23
	__GETD2N 0x3D090
	CALL __DIVD21
	RJMP _0xDA
; 0000 01CC         }
; 0000 01CD         else if (x>=500)
_0x5F:
	LD   R26,Y
	LDD  R27,Y+1
	CPI  R26,LOW(0x1F4)
	LDI  R30,HIGH(0x1F4)
	CPC  R27,R30
	BRLO _0x61
; 0000 01CE         { TCCR0=0x1C; TCNT0=0;
	LDI  R30,LOW(28)
	OUT  0x33,R30
	LDI  R30,LOW(0)
	OUT  0x32,R30
; 0000 01CF           OCR0 = 125000 / x;
	LD   R30,Y
	LDD  R31,Y+1
	CLR  R22
	CLR  R23
	__GETD2N 0x1E848
	CALL __DIVD21
	RJMP _0xDA
; 0000 01D0         }
; 0000 01D1         else if (x>=250)
_0x61:
	LD   R26,Y
	LDD  R27,Y+1
	CPI  R26,LOW(0xFA)
	LDI  R30,HIGH(0xFA)
	CPC  R27,R30
	BRLO _0x63
; 0000 01D2         { TCCR0=0x1D; TCNT0=0;
	LDI  R30,LOW(29)
	OUT  0x33,R30
	LDI  R30,LOW(0)
	OUT  0x32,R30
; 0000 01D3           OCR0 = 67500 / x;
	LD   R30,Y
	LDD  R31,Y+1
	CLR  R22
	CLR  R23
	__GETD2N 0x107AC
	CALL __DIVD21
	RJMP _0xDA
; 0000 01D4         }
; 0000 01D5         else
_0x63:
; 0000 01D6         { TCCR0=0x1E; TCNT0=0;
	LDI  R30,LOW(30)
	OUT  0x33,R30
	LDI  R30,LOW(0)
	OUT  0x32,R30
; 0000 01D7           OCR0 = 33750 / x;
	LD   R30,Y
	LDD  R31,Y+1
	LDI  R26,LOW(33750)
	LDI  R27,HIGH(33750)
	CALL __DIVW21U
_0xDA:
	OUT  0x31,R30
; 0000 01D8         }
; 0000 01D9   }
; 0000 01DA   else
	RJMP _0x65
_0x5B:
; 0000 01DB   { BuzzerOff();
	RCALL _BuzzerOff
; 0000 01DC   }
_0x65:
; 0000 01DD }
	RJMP _0x2000001
;#pragma used-
;
;#pragma used+
;void Nada(int Frek, unsigned char Tempo)
; 0000 01E2 {  FBuzzer(Frek); delay_ms((char)(Tempo*10));}
;	Frek -> Y+1
;	Tempo -> Y+0
;#pragma used-
;
;// -----------------  Fungsi Servo ----------------------------
;#pragma used+
;unsigned char SetSudut(unsigned int sudut)  // perubahan terkecil = 180/40 = 4,5 derajat
; 0000 01E8 {   return ((sudut*4)/18 + 40);}
_SetSudut:
;	sudut -> Y+0
	LD   R26,Y
	LDD  R27,Y+1
	LDI  R30,LOW(4)
	CALL __MULB1W2U
	MOVW R26,R30
	LDI  R30,LOW(18)
	LDI  R31,HIGH(18)
	CALL __DIVW21U
	SUBI R30,-LOW(40)
	RJMP _0x2000001
;#pragma used-
;
;#pragma used+
;unsigned int SetSudut2(unsigned int sudut) // perubahan terkecil = 180/2000 = 0,09 derajat
; 0000 01ED {   return ((sudut*100)/9 + 2000);}
_SetSudut2:
;	sudut -> Y+0
	LD   R26,Y
	LDD  R27,Y+1
	LDI  R30,LOW(100)
	CALL __MULB1W2U
	MOVW R26,R30
	LDI  R30,LOW(9)
	LDI  R31,HIGH(9)
	CALL __DIVW21U
	SUBI R30,LOW(-2000)
	SBCI R31,HIGH(-2000)
	RJMP _0x2000001
;#pragma used-
;
;#pragma used+
;void SudutServo1(unsigned char posisi)
; 0000 01F2 {   dServo1 = SetSudut(posisi);}
_SudutServo1:
;	posisi -> Y+0
	LD   R30,Y
	LDI  R31,0
	ST   -Y,R31
	ST   -Y,R30
	RCALL _SetSudut
	STS  _dServo1,R30
	RJMP _0x2000004
;#pragma used-
;#pragma used+
;void SudutServo2(unsigned char posisi)
; 0000 01F6 {   dServo2 = SetSudut(posisi);}
_SudutServo2:
;	posisi -> Y+0
	LD   R30,Y
	LDI  R31,0
	ST   -Y,R31
	ST   -Y,R30
	RCALL _SetSudut
	STS  _dServo2,R30
	RJMP _0x2000004
;#pragma used-
;#pragma used+
;void SudutServo3(unsigned char posisi)
; 0000 01FA {   dServo3 = SetSudut(posisi);}
_SudutServo3:
;	posisi -> Y+0
	LD   R30,Y
	LDI  R31,0
	ST   -Y,R31
	ST   -Y,R30
	RCALL _SetSudut
	STS  _dServo3,R30
	RJMP _0x2000004
;#pragma used-
;#pragma used+
;void SudutServo4(unsigned char posisi)
; 0000 01FE {   dServo4 = SetSudut(posisi);}
_SudutServo4:
