	ST   -Y,R30
	LDI  R30,LOW(0)
	ST   -Y,R30
	RCALL _LCD_GotoXY
	LDS  R30,_dServo2
	LDI  R31,0
	ST   -Y,R31
	ST   -Y,R30
	RCALL _LCD_Angka3
